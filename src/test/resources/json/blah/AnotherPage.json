{
  "name": "AnotherPage",
  "defaultLocale": "en_US",
  "elements": [
    {
      "name": "heading",
      "type": "label",
      "locale": [
        {
          "name": "en_US",
          "locator": "css=h1[class=heading]"
        }
      ],
      "wait": {
        "until": "visible",
        "for": 10
      }
    },
    {
      "name": "checkboxesLink",
      "list": true,
      "type": "link",
      "locale": [
        {
          "name": "en_US",
          "locator": "xpath=//a[text()='Checkboxes']"
        }
      ],
      "wait": {
        "until": "clickable",
        "for": 10
      }
    },
    {
      "name": "sampleTxtField",
      "type": "text",
      "locale": [
        {
          "name": "en_US",
          "locator": "xpath=//a[text()='textField']"
        }
      ]
    }
  ]
}
